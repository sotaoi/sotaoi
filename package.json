{
  "name": "@sotaoi/sotaoi",
  "private": true,
  "scripts": {
    "set:ecosystem": "node ./scripts/set/set-ecosystem",
    "set:super-api": "node ./scripts/set/set-super-api",
    "set:api": "node ./scripts/set/set-api",
    "set:x-api": "node ./scripts/set/set-x-api",
    "set:client": "node ./scripts/set/set-client",
    "set:web": "node ./scripts/set/set-web",
    "set:mobile": "node ./scripts/set/set-mobile",
    "set:desktop": "node ./scripts/set/set-desktop",
    "bootstrap": "node ./scripts/bootstrap",
    "reset": "npm run clean:project && npm run bootstrap",
    "adb:devices": "node ./scripts/adb/adb-devices",
    "install:certs": "node ./scripts/install/install-certs",
    "install:rnsdk": "node ./scripts/install/install-rnsdk",
    "install:pull": "node ./scripts/install/install-pull",
    "db:migrate": "node ./scripts/db/db-migrate",
    "db:rollback": "node ./scripts/db/db-rollback",
    "db:migrate:down": "node ./scripts/db/db-migrate-down",
    "db:seed": "node ./scripts/db/db-seed",
    "start:servers": "concurrently --kill-others \"npm run detach:proxy\" \"npm run start:api\" \"npm run start:auth\" \"npm run start:streaming\"",
    "start:servers:prod": "node ./scripts/start/start-servers-prod",
    "restart:servers:prod": "npm run stop:servers:prod && npm run start:servers:prod",
    "stop:servers:prod": "node ./scripts/stop/stop-servers-prod",
    "start:api": "node ./scripts/start/start-api",
    "start:api:prod": "node ./scripts/start/start-api-prod",
    "restart:api:prod": "node ./scripts/restart/restart-api-prod",
    "stop:api:prod": "node ./scripts/stop/stop-api-prod",
    "start:auth": "node ./scripts/start/start-auth",
    "start:auth:prod": "node ./scripts/start/start-auth-prod",
    "restart:auth:prod": "node ./scripts/restart/restart-auth-prod",
    "stop:auth:prod": "node ./scripts/stop/stop-auth-prod",
    "start:proxy": "node ./scripts/start/start-proxy",
    "detach:proxy": "node ./scripts/detach/detach-proxy",
    "restart:proxy": "node ./scripts/restart/restart-proxy",
    "stop:proxy": "node ./scripts/stop/stop-proxy",
    "start:proxy:prod": "node ./scripts/start/start-proxy-prod",
    "restart:proxy:prod": "node ./scripts/restart/restart-proxy-prod",
    "stop:proxy:prod": "node ./scripts/stop/stop-proxy-prod",
    "start:streaming": "node ./scripts/start/start-streaming",
    "start:streaming:prod": "node ./scripts/start/start-streaming-prod",
    "restart:streaming:prod": "node ./scripts/restart/restart-streaming-prod",
    "stop:streaming:prod": "node ./scripts/stop/stop-streaming-prod",
    "start:php": "node ./scripts/start/start-php",
    "start:web": "node ./scripts/start/start-web",
    "start:web:prod": "node ./scripts/start/start-web-prod",
    "stop:web:prod": "node ./scripts/stop/stop-web-prod",
    "start:rn": "node ./scripts/start/start-rn",
    "start:ios": "node ./scripts/start/start-ios",
    "start:android": "node ./scripts/start/start-android",
    "build:web": "node ./scripts/build/build-web",
    "clean:project": "node ./scripts/clean/clean-project",
    "clean:web": "node ./scripts/clean/clean-web",
    "clean:mobile": "node ./scripts/clean/clean-mobile",
    "clean:ios": "node ./scripts/clean/clean-ios",
    "clean:android": "node ./scripts/clean/clean-android",
    "push:all": "node ./scripts/push/push-all",
    "pull:all": "node ./scripts/pull/pull-all",
    "status:all": "node ./scripts/status/status-all",
    "shell:clear-mc": "node ./scripts/shell/shell-clear-mc",
    "shell:rnr": "npm run shell:clear-mc && npm run start:rn",
    "app:up": "node ./scripts/app/app-up",
    "app:down": "node ./scripts/app/app-down",
    "force:remove": "node ./scripts/force/force-remove",
    "postinstall": "node ./scripts/hook/hook-postinstall.js",
    "pkill": "pkill -f 'npm run start'",
    "ssl:greenlock": "node ./scripts/ssl/ssl-greenlock",
    "ssl:greenlock:prod": "node ./scripts/ssl/ssl-greenlock-prod",
    "ssl:snakeoil": "node ./scripts/ssl/ssl-snakeoil",
    "shell:tail": "concurrently \"tail -f ./app-api/logs/*.log\" \"tail -f ./app-auth/logs/*.log\" \"tail -f ./app-php/logs/apache/*.log\" \"tail -f ./app-php/storage/logs/*.log\" \"tail -f ./app-proxy/logs/*.log\" \"tail -f ./app-streaming/logs/*.log\" \"tail -f ./app-web/server-output.log\""
  },
  "dependencies": {
    "@types/node": "^14",
    "command-exists": "^1.2.9",
    "concurrently": "^6.2.0"
  }
}
